TARGETTYPE=LIBRARY
TARGETNAME=ceshapi

PREPROCESSDEFFILE=1
TARGETDEFNAME=ceshell
DEFFILE=$(TARGETDEFNAME).def
WINCETARGETFILE0=$(_RELEASELIBDIR)\$(DEFFILE)

RCADDNULL=1
WINCETARGETFILES=$(_RELEASELIBDIR)\$(TARGETNAME)_base.res \
                 $(_RELEASELIBDIR)\$(TARGETNAME)_vgal.res \
                 $(_RELEASELIBDIR)\$(TARGETNAME)_qvgap.res

WINCEOEM=1
WARNLEVEL=4

INCLUDES=..\inc;..\..\inc;..\ui

SOURCES=api.cpp \
        browse.cpp \
        cundo.cpp \
        drag.cpp \
        fileopen.cpp \
        iconcache.cpp \
        idlist.cpp \
        malloc.cpp \
        shfileop.cpp \
        shortcut.cpp \
        recbin.cpp
