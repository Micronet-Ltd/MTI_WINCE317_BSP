TARGETNAME=btmodem
TARGETTYPE=DYNLINK
RELEASETYPE=PLATFORM
SYNCHRONIZE_DRAIN=1

DLLENTRY=_DllMainCRTStartup
CDEFINES=$(CDEFINES) -DCEDDK_USEDDKMACRO

TARGETDEFNAME=$(TARGETNAME)
DEFFILE=$(TARGETDEFNAME).def

SOURCES=$(TARGETNAME).cxx

