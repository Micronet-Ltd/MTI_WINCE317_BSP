	$(_TARGETPLATROOT)\SRC\SDK\INC;

TARGETLIBS= \
    $(_PLATCOMMONLIB)\$(_CPUINDPATH)\ceddk_omap35xx_tps659xx_ti_v1.lib \
    $(_COMMONSDKLIB)\$(_CPUINDPATH)\coredll.lib \
    $(_PROJECTROOT)\cesysgen\oak\lib\$(_CPUINDPATH)\btdrt.lib \
    $(_PROJECTROOT)\cesysgen\oak\lib\$(_CPUINDPATH)\ceosutil.lib \
    $(_TARGETPLATLIB)\$(_CPUDEPPATH)\bsp_drvlib.lib \
    $(_PROJECTSDKROOT)\lib\$(_CPUDEPPATH)\MicUserSdk.lib
