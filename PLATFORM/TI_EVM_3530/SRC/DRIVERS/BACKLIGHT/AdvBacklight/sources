SKIPBUILD=1
!ENDIF

WINCEOEM=1
SYNCHRONIZE_DRAIN=1
PREPROCESSDEFFILE=1
TARGETNAME=ADVBACKLIGHT
RELEASETYPE=PLATFORM
TARGETTYPE=DYNLINK
DLLENTRY=_DllEntryCRTStartup
DEFFILE=$(TARGETNAME).def

TARGETLIBS= \
    $(_SYSGENSDKROOT)\lib\$(_CPUINDPATH)\coredll.lib \
    $(_SYSGENSDKROOT)\lib\$(_CPUINDPATH)\commctrl.lib \

	
INCLUDES= $(_TARGETPLATROOT)\SRC\INC; $(_WINCEROOT)\public\common\sdk\inc


SOURCES= \
  AdvBacklight.cpp \
  Sliders.cpp \
  $(TARGETNAME).rc

FILE_VIEW_INCLUDES_FOLDER= \
	AdvBacklight.h \
	Sliders.h \
