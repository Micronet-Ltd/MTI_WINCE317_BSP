STYLE WS_OVERLAPPED | WS_VISIBLE | WS_CLIPSIBLINGS | DS_CENTER | WS_SYSMENU
EXSTYLE WS_EX_CAPTIONOKBTN
CAPTION "Ignition Switch Control"
BEGIN
    CONTROL         "Enable Ignition Switch Control",IDC_IGNI_ENABLE,"Button",BS_AUTOCHECKBOX | WS_TABSTOP,13,6,110,10
