    COMBOBOX        IDC_ACT,107,40,69,30,CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    COMBOBOX        IDC_ACT_DELAY,107,57,69,80,CBS_DROPDOWNLIST | WS_VSCROLL | WS_TABSTOP
    LTEXT           "Ignition Stable Time:",IDC_STATIC,13,27,69,10
    LTEXT           "Action Delay:",IDC_STATIC,13,61,66,10
    LTEXT           "Perform Action:",IDC_STATIC,13,44,66,10
END

