!IF "$(BSP_NO_IGNITION)" == "1"
SKIPBUILD=1
!ENDIF

CPL=1
TARGETNAME=IgnitionCpl
RELEASETYPE=PLATFORM
WINCEOEM=1

TARGETTYPE=DYNLINK
COPYRES=1
DLLENTRY=DllEntry

TARGETDEFNAME=$(TARGETNAME)
WINCETARGETFILES=$(_RELEASELIBDIR)\$(TARGETNAME).res
WINCETARGETFILE0=$(_RELEASELIBDIR)\$(TARGETDEFNAME).def

TARGETLIBS=$(TARGETLIBS) \
	$(_COMMONSDKLIB)\$(_CPUINDPATH)\commctrl.lib \
    $(_PLATCOMMONLIB)\$(_CPUINDPATH)\ceddk_omap35xx_tps659xx_ti_v1.lib \
    $(_COMMONSDKLIB)\$(_CPUINDPATH)\coredll.lib

INCLUDES= $(INCLUDES); $(_TARGETPLATROOT)\SRC\INC; $(_PUBLICROOT)\common\sdk\inc



SOURCES=IgnitionCpl.cpp IgnitionCpl.rc
