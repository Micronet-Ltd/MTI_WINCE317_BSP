                     $(_TARGETPLATROOT)\SRC\boot\eboot\LDR\;\
			         $(_TARGETPLATROOT)\SRC\BOOT\EXFATFS; \
			         $(_TARGETPLATROOT)\SRC\BOOT\FALLITE; \
                     $(_PUBLICROOT)\common\oak\DRIVERS\ETHDBG\EBOOT;\                        
                     
INCLUDES=$(INCLUDES);..

